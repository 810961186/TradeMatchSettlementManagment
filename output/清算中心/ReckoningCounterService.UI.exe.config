		</services>
		<bindings>
			<netTcpBinding>
				<binding name="normalBinding" closeTimeout="00:02:00"
					openTimeout="00:02:00" receiveTimeout="23:59:59" sendTimeout="23:59:59"
					transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions"
					hostNameComparisonMode="StrongWildcard" listenBacklog="10"
					maxBufferPoolSize="5242880000" maxBufferSize="655360000" maxConnections="1000"
					maxReceivedMessageSize="655360000">
					<readerQuotas maxDepth="32" maxStringContentLength="8192000" maxArrayLength="16384000"
						maxBytesPerRead="4096000" maxNameTableCharCount="16384000" />
					<reliableSession ordered="true" inactivityTimeout="00:10:00"
						enabled="false" />
					<security mode="None" />
				</binding>
			</netTcpBinding>
		</bindings>
		<behaviors>
			<serviceBehaviors>
				<behavior name="MEX">
					<serviceMetadata/>
					<serviceThrottling maxConcurrentCalls="6400" maxConcurrentSessions="1000"/>
					<serviceDebug includeExceptionDetailInFaults="true"/>
					<dataContractSerializer maxItemsInObjectGraph="655360000"/>
				</behavior>
			</serviceBehaviors>
			<endpointBehaviors>
				<behavior name="clientBehavior">
					<dataContractSerializer maxItemsInObjectGraph="655360000" />
				</behavior>
			</endpointBehaviors>
		</behaviors>
		<client>
			<endpoint address="" binding="netTcpBinding"
				bindingConfiguration="normalBinding" contract="MatchCenterService.IDoOrder"
				name="NetTcpBinding_IDoOrder" />
			<endpoint address="" binding="netTcpBinding"
				bindingConfiguration="normalBinding" contract="MatchCenterOrderDealRpt.IOrderDealRpt"
				name="NetTcpBinding_IOrderDealRpt" />
			<endpoint address="net.tcp://localhost:9381/WcfCommonalityProvider"
				binding="netTcpBinding" bindingConfiguration="normalBinding"
				contract="DevolveVerifyCommonService.ICommonPara" name="NetTcpBinding_ICommonPara" />
			<endpoint address="net.tcp://localhost:9381/WcfFuturesProvider"
				binding="netTcpBinding" bindingConfiguration="normalBinding"
				contract="FuturesDevolveService.IFuturesTradeRules" name="NetTcpBinding_IFuturesTradeRules" />
			<endpoint address="net.tcp://localhost:9381/WcfSpotTradingRulesProvider"
				binding="netTcpBinding" bindingConfiguration="normalBinding"
				contract="SpotTradingDevolveService.ISpotTradeRules" name="NetTcpBinding_ISpotTradeRules" />
			<endpoint address="net.tcp://localhost:9381/WcfTransactionManageProvider"
				binding="netTcpBinding" bindingConfiguration="normalBinding"
				contract="TransactionManageService.ITransactionManage" name="NetTcpBinding_ITransactionManage" />
      <endpoint address="net.tcp://localhost:9381/WcfHKTradingRulesProvider"
          binding="netTcpBinding" bindingConfiguration="normalBinding"
          contract="HKTradingRulesService.IHKTradeRules" name="NetTcpBinding_IHKTradeRules" />
		</client>
	</system.serviceModel>
	<appSettings>
		<!--盘后清算延迟的时间（分钟）-->
		<add key="reckondelaytime" value="30"/>

		<!--柜台标识（必须保证唯一）-->
		<add key="CounterID" value="一号柜台" />

		<!--报盘线程数量-->
		<add key="offerThread" value="200"/>

		<!--清算线程数量-->
		<add key="dealThread" value="200"/>

		<!--定时清算间隔时间（毫秒）-->
		<add key="ReckonCommitInterval" value="2000"/>

		<!--实时行情组件类别 1-socket 2-fast-->
		<add key="realTimeMode" value="1"/>

		<!--每日开盘资金检查延迟时间-->
		<add key="daycheck" value="30"/>

		<!--启动时是否加载全部持仓数据，1-fullload 2-delayload-->
		<add key="loadAllData" value="1"/>

	</appSettings>
</configuration>