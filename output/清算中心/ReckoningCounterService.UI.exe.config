<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
		<section name="dataConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Data.Configuration.DatabaseSettings, Microsoft.Practices.EnterpriseLibrary.Data" />
	</configSections>
	<connectionStrings>
		<add name="ReckonConnectionString" providerName="System.Data.SqlClient"
			 connectionString="Data Source=JACKY-PC\REAGTORDB;Initial Catalog=VTS_ReckoningCounter_V3.0;user id=sa;password=P@ssw0rd;Max Pool Size=200;Min Pool Size=5;Pooling=true;Connection Timeout=50" />
	</connectionStrings>
	<dataConfiguration defaultDatabase="ReckonConnectionString"/>
	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender" >
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level - %message%newline" />
			</layout>
		</appender>

		<appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Log\server.log" />
			<appendToFile value="true" />
			<maximumFileSize value="2000KB" />
			<maxSizeRollBackups value="200" />

			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level - %message%newline" />
			</layout>
		</appender>

		<appender name="CapitalFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Log\Capital.log" />
			<appendToFile value="true" />
			<maximumFileSize value="2000KB" />
			<maxSizeRollBackups value="200" />

			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level - %message%newline" />
			</layout>
		</appender>

		<!-- level:ERROR INFO DEBUG OFF -->
		<root>
			<!--<level value="DEBUG" />
			<appender-ref ref="FileAppender" />
			<appender-ref ref="ConsoleAppender" />-->
		</root>
		<!--主日志-->
		<logger name="Server" >
			<level value="DEBUG" />
			<appender-ref ref="FileAppender" />
			<appender-ref ref="ConsoleAppender" />
		</logger>
		<logger name="Capital">
			<level value="DEBUG" />
			<appender-ref ref="CapitalFileAppender" />
			<appender-ref ref="ConsoleAppender" />
		</logger>

	</log4net>
	<system.serviceModel>
		<services>

			<service name="ReckoningCounter.BLL.DoOrderService"  behaviorConfiguration="MEX" >
